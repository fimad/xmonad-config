Config { font = "xft:Inconsolata for Powerline:size=11:antialias=true:hinting=full"
       , additionalFonts = []
       , bgColor = "#002b36"
       , fgColor = "#839496"
       , border = TopBM -1
       , borderColor = "#002b36"
       , borderWidth = 3
       , position = TopW L 96
       , lowerOnStart = False
       , commands = [	Run StdinReader
       							,	Run Memory ["-t","ram <usedratio>%"] 10
                    , Run Date "%b %_d %Y %I:%M:%S %p" "date" 10
                    , Run Com "xmobar_pulse.sh" [] "vol" 10
                    ]
       , sepChar = "%"
       , alignSep = "}{"
       , template =
       "<fc=#fdf6e3,#93a1a1>%StdinReader%</fc>}{<fc=#2aa198,#002b36></fc>%vol%<fc=#268bd2,#2aa198></fc><fc=#fdf6e3,#268bd2>%memory%</fc><fc=#93a1a1,#268bd2></fc><fc=#fdf6e3,#93a1a1> %date% </fc><fc=#002b36,#93a1a1></fc>"
       }
